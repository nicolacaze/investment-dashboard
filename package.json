{
  "name": "investment-dashboard",
  "version": "1.0.0",
  "description": "A personal dashboard to track my investments.",
  "default": "index.html",
  "scripts": {
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "dev": "parcel src/index.html",
    "build": "yarn test && parcel build src/index.html",
    "test": "jest --watch"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "./src/setupTests.js"
    ]
  },
  "author": "Nicolas Lacaze",
  "license": "ISC",
  "dependencies": {
    "formik": "^2.2.9",
    "gotrue-js": "^0.9.29",
    "node-fetch": "2.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "read-excel-file": "^5.2.20",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@parcel/babel-preset-env": "^2.0.0",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^27.3.1",
    "eslint": "^8.1.0",
    "eslint-plugin-jest": "^25.2.3",
    "eslint-plugin-react": "^7.26.1",
    "jest": "^27.3.1",
    "parcel": "^2.0.0",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "tailwindcss": "^2.2.19"
  }
}
